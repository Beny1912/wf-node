{
  "name": "wf-node",
  "version": "1.0.0",
  "description": "Challenge to wefox Architect Lead offer",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "prestart": "node ./npm-scripts/prestart.js pro",
    "start": "node .",
    "start-dev": "node ./npm-scripts/prestart.js dev && nodemon ./src/index.ts",
    "start-pro": "node ./npm-scripts/prestart.js pro && nodemon ./src/index.ts",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint \"./src/**/*.ts\" --fix",
    "prettier": "prettier --write .",
    "prepack": "node ./npm-scripts/prestart.js pro && tsc",
    "pack": "pkg .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beny1912/wf-node.git"
  },
  "author": "Beny Pe√±a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Beny1912/wf-node/issues"
  },
  "homepage": "https://github.com/Beny1912/wf-node#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "express": "^4.17.2"
  },
  "pkg": {
    "targets": [
      "node14-win-x64"
    ],
    "assets": [
      ".env",
      "./dist/**/*",
      "./libs/**/*",
      "./node_modules/**/*"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "libs",
      "node_modules"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
