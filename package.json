{
  "name": "wf-node",
  "version": "1.0.0",
  "description": "Challenge to wefox Architect Lead offer",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "prestart": "node ./npm-scripts/prestart.js pro",
    "start": "node .",
    "start-dev": "node ./npm-scripts/prestart.js dev && nodemon ./src/index.ts",
    "start-pro": "node ./npm-scripts/prestart.js pro && nodemon ./src/index.ts",
    "build": "tsc --skipLibCheck",
    "test": "jest --passWithNoTests",
    "eslint": "eslint './src/**/*.ts'",
    "eslint:fix": "eslint --fix './src/**/*.ts'",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "prepack": "node ./npm-scripts/prestart.js pro && tsc",
    "pack": "pkg .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beny1912/wf-node.git"
  },
  "author": "Beny Pe√±a",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Beny1912/wf-node/issues"
  },
  "homepage": "https://github.com/Beny1912/wf-node#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "cd": "^0.3.3",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^6.6.6",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.5",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.10.5",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "mongoose": "^6.1.5"
  },
  "pkg": {
    "targets": [
      "node14-win-x64"
    ],
    "assets": [
      ".env",
      "./dist/**/*",
      "./libs/**/*",
      "./node_modules/**/*"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "libs",
      "node_modules"
    ],
    "roots": [
      "./__tests__"
    ],
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
